        <key>gotoDefinitionLinkForeground</key><!-- VSC: ?? -->
        <string>#29BEEA</string>

        <key>gutter</key>
        <string>#F5F5F5</string>
        <key>gutterForeground</key>
        <string>#B9B9B9</string>

        <key>guide</key><!-- The sidebar for parts that can be expanded/contracted -->
